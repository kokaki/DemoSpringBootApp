<!DOCTYPE configuration>
<FindBugsFilter>
	<Match>
		<Class name="~.*\.*Test" />
		<Not>
			<Bug code="IJU" />
		</Not>
	</Match>
	<Match>
		<Bug pattern="EI_EXPOSE_REP" />
	</Match>
	<Match>
		<Bug pattern="EI_EXPOSE_REP2" />
	</Match>
	<Match>
		<Bug pattern="SS_SHOULD_BE_STATIC" />
	</Match>
	<Match>
		<Bug pattern="UWF_UNWRITTEN_FIELD" />
	</Match>
</FindBugsFilter>
